<h1>Главная страница</h1>
<%# <%= @db_result %> 
<table class="table">
  <% @db_result.each do |item| %>
    <tr>
      <td>
        <br>
        <%= item['content_db'] %>
        <br>
        <br>
        <span>Добавлен в: </span><%= item['date_create'] %>
        <br>
        <a href="/details/<%= item['id'] %>">Комментарий</a>
      </td>
    </tr>
  <% end %>
</table>
